                                <td data-field="startDate" class="start-date hidden"> {{ item.match.StartTime}}</td>
                                <td class="livebet"><span class="badge">Live</span></td>
                                <td>1+</td>
                                <td data-field="teamVersus" class="team-versus">{{ item.match.HomeTeamName }} vs  {{ item.match.AwayTeamName }}</td>
                                <td><input type="button" class="btn btn-default btn-xs odd " value="{{item.oddgg.Odd}}" data-odd-type="{{item.oddgg.BetOptionId }}" data-option-id="{{ item.oddgg.BetOptionId }}" data-option-name="{{item.oddgg.BetOption }}" data-bet-category="{{item.oddgg.BetCategory }}" /></td>
                                <td><input type="button" class="btn btn-default btn-xs odd " value="{{item.oddng.Odd}}" data-odd-type="{{ item.oddng.BetOptionId }}" data-option-id="{{ item.oddng.BetOptionId }}" data-option-name="{{ item.oddng.BetOption }}" data-bet-category="{{ item.oddng.BetCategory }}" /></td>
                                <td class="livebet"><input type="button" class="btn btn-primary btn-xs moreodds" value="+{{ item.TotalOdds }}" /></td>
                            </tr>
                        </tbody>
                    </table>
                </script>
